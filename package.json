{
  "name": "youtubesave",
  "version": "1.0.0",
  "description": "A simple youtube downloader build with Fastify",
  "main": "./dist/index.js",
  "scripts": {
    "sass": "sass --watch public/test.scss:public/test.css",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "dev": "nodemon ./index.ts && npm run sass",
    "build": "tsc -p tsconfig.json && cp ./cert.pem ./dist && cp ./key.pem ./dist",
    "preview": "tsc && node ./dist/index.js",
    "start": "node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tran Nguyen Thuong Truong",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/rate-limit": "^8.0.1",
    "@fastify/static": "^6.10.2",
    "fastify": "^4.19.1",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.22",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}