{
    "defaultServerity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-plugin-airbnb"
    ],
    "rules": {
        "trailing-comma": [
            false
        ],
        "import-name": [
            false
        ],
        "variable-name": [
            false
        ],
        "function-name": [
            false
        ],
        "no-unused-expression": [
            true
        ]
    }
}